{
    "manifest_version": 2,

    "name": "Chronus",
    "description": "This extension is for a persons DigitalWellbeing",
    "version": "1.0",
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },

    "background": {
        "scripts": ["js/background.js" , "js/jquery.min.js"]
    },

    "chrome_url_overrides":{
        "newtab" : "index.html"
    },

    "permissions": [
        "tabs",
        "storage"
    ],

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
